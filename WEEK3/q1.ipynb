    "\n",
    "def invertdict(d):\n",
    " return {v: k for k, v in d.items()}\n",
    " \n",
    "data={\"a\":1,\"b\":2,\"c\":3}\n",
    "inverted=invertdict(data)\n",
    "print(\"original dictionary: \",data)\n",
    "print(\"duplicate dictionary:\",inverted)"
   ]
  }
